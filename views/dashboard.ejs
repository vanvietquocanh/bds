                    <div class="container">

                        <div class="row">
<%var dateNow = new Date()%>
<%var beginDayNow = `${dateNow.getDate()}/${dateNow.getMonth()+1}/${dateNow.getFullYear()}`%>
<%var beginMonthNow = `${dateNow.getMonth()+1}/${dateNow.getFullYear()}`%>
<%var countConnectDay = 0, countConnectMonth = 0, countConnectYear = 0%>
<%for (var i = 0; i < result.length; i++) {%>
    <%countConnectYear+=result[i].count%>
    <%if(result[i]._id.day===beginDayNow){%>
        <%countConnectDay = parseFloat(result[i].count)%>
    <%}%>
    <%if(result[i]._id.day===beginMonthNow){%>
        <%countConnectMonth = result[i].count%>
    <%}%>
<%}%>
                            <div class="col-lg-3 col-md-6">
                        		<div class="card-box">

                        			<h4 class="header-title m-t-0 m-b-30">Tỉ lệ truy cập /ngày</h4>

                                    <div class="widget-chart-1">
                                        <div class="widget-chart-box-1">
                                            <input data-plugin="knob" data-width="80" data-height="80" data-fgColor="#f05050 "
                                               data-bgColor="#F9B9B9" value="<%-countConnectDay%>"
                                               data-skin="tron" data-angleOffset="180" data-readOnly=true
                                               data-thickness=".15"/>
                                        </div>

                                        <div class="widget-detail-1">
                                            <h2 class="p-t-10 m-b-0"> <%-countConnectDay%> </h2>
                                            <p class="text-muted">Lượt truy cập trong tháng</p>
                                        </div>
                                    </div>
                        		</div>
                            </div><!-- end col -->

                            <div class="col-lg-3 col-md-6">
                        		<div class="card-box">

                        			<h4 class="header-title m-t-0 m-b-30">Lượt truy cập trong năm</h4>

                                    <div class="widget-box-2">
                                        <div class="widget-detail-2">
                                            <h2 class="m-b-0"> <%-countConnectYear%> </h2>
                                            <p class="text-muted m-b-25"><%-countConnectYear%>/10000</p>
                                        </div>
                                        <div class="progress progress-bar-success-alt progress-sm m-b-0">
                                            <div class="progress-bar progress-bar-success" role="progressbar"
                                                 aria-valuenow="<%-parseFloat(countConnectYear/10000)%>" aria-valuemin="0" aria-valuemax="100"
                                                 style="width: <%-parseFloat(countConnectYear/10000)%>%;">

                                            </div>
                                        </div>
                                    </div>
                        		</div>
                            </div><!-- end col -->

                            <div class="col-lg-3 col-md-6">
                        		<div class="card-box">

                        			<h4 class="header-title m-t-0 m-b-30">Tổng số Email</h4>

                                    <div class="widget-chart-1">
                                        <div class="widget-chart-box-1">
                                            <input data-plugin="knob" data-width="80" data-height="80" data-fgColor="#ffbd4a"
                                               data-bgColor="#FFE6BA" value="<%-countEmail/1000%>"
                                               data-skin="tron" data-angleOffset="180" data-readOnly=true
                                               data-thickness=".15"/>
                                        </div>
                                        <div class="widget-detail-1">
                                            <h2 class="p-t-10 m-b-0"> <%-countEmail%> </h2>
                                            <p class="text-muted">Email</p>
                                        </div>
                                    </div>
                        		</div>
                            </div><!-- end col -->

                            <div class="col-lg-3 col-md-6">
                        		<div class="card-box">

                        			<h4 class="header-title m-t-0 m-b-30">Tổng bất động sản trên trang</h4>

