                                        <div class="widget-detail-2">
                                            <h2 class="m-b-0"> <%-countArea%> </h2>
                                            <p class="text-muted m-b-25"><%-countArea%>/1000</p>
                                        </div>
                                        <div class="progress progress-bar-pink-alt progress-sm m-b-0">
                                            <div class="progress-bar progress-bar-pink" role="progressbar"
                                                 aria-valuenow="<%-countVideo/1000%>" aria-valuemin="0" aria-valuemax="100"
                                                 style="width: <%-countArea/1000%>%;">
                                                <span class="sr-only"><%-countArea/1000%>% Complete</span>
                                            </div>
                                        </div>
                                    </div>
                        		</div>
                            </div><!-- end col -->
                            <div class="col-lg-3 col-md-6">
